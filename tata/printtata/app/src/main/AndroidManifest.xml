<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.desarrollo.printata" >

    <uses-feature android:name="android.hardware.usb.host" />

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/m_logo_sanroque"
        android:supportsRtl="true"
        android:theme="@style/AppTheme5" >
        <activity
            android:name="com.desarrollo.printata.consultor.fullscream.FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name="com.desarrollo.printata.consultor.ConsultorPrecioActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity
            android:name="com.desarrollo.printata.ScannerElo"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.desarrollo.printata.Configuracion.Registro_Grupo_Rubros"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.desarrollo.printata.ListTagGondola"
            android:label="Cambio de precio" />
        <activity
            android:name="com.desarrollo.printata.Flash"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:windowSoftInputMode="stateHidden|adjustResize" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.desarrollo.printata.TagGondola"
            android:configChanges="screenSize|orientation"
            android:label="Tag de GÃ³ndola"
            android:theme="@style/AppTheme5"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.desarrollo.printata.Configuracion.DOPrintMainActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|screenSize"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.desarrollo.printata.Configuracion.ConnectionSettingsActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden|adjustResize" >
            <intent-filter>
                <action android:name="com.desarrollo.myapplication.ConnectionSettingsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.desarrollo.printata.MenuPrincipal"
            android:label="@string/consola"
            android:theme="@style/AppTheme5.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>